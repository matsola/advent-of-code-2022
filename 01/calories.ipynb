    "def read_elves():\n",
    "    i = 0\n",
    "    for line in open(\"calories.txt\"):\n",
    "        if line == '\\n':\n",
    "            yield i\n",
    "            i = 0\n",
    "        else:\n",
    "            i += int(line)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "max(read_elves())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
